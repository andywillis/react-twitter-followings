  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.9",
    "@babel/plugin-syntax-import-assertions": "^7.18.6",
    "concurrently": "^7.4.0",
    "eslint": "^8.23.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "^2.26.0"
  }
}
